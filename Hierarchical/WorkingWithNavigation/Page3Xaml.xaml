<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    x:Class="WorkingWithNavigation.Page3Xaml"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Page 3">
    <ContentPage.Content>
        <StackLayout>
            <Button
                x:Name="previousButton"
                Clicked="OnPreviousPageButtonClicked"
                Text="Previous Page"
                VerticalOptions="CenterAndExpand" />
            <Button
                x:Name="rootButton"
                Clicked="OnRootPageButtonClicked"
                Text="Return to Root Page"
                VerticalOptions="CenterAndExpand" />
            <Button
                x:Name="insertButton"
                Clicked="OnInsertPageButtonClicked"
                Text="Insert Page 2a Before Page 3"
                VerticalOptions="CenterAndExpand" />
            <Button
                x:Name="removeButton"
                Clicked="OnRemovePageButtonClicked"
                Text="Remove Page 2"
                VerticalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
